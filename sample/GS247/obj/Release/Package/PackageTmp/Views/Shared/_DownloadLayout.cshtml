
@{
    ViewBag.Title = "_DownloadLayout";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
<style>
    #othleft-sidebar ul li a:hover {
        color: #50d8af;
    }
</style>
 </head>
<body style="background-color:">

    <div class="wrap_container_bg navbar-fixed-custom navbar-custom" id="navbarMain">
        <div class="wrap_container_admin wrap_container_flex">
            <script>
                $(document).ready(function () {
                    window.history.pushState(null, "", window.location.href);
                    window.onpopstate = function () {
                        window.history.pushState(null, "", window.location.href);
                    };
                });
                function setDefault(Id) {
                    var tag = document.getElementById(Id);
                    tag.setAttribute("class", "list_active");
                }
            </script>

            <div class="midnav" style="background-color: ; color: ">
                <div class="breadcrumbs">

                </div>
            </div>
            <div class="container">
                <div class="container">
                    <div class="span-19">
                        <div id="content">

                            <table width="100%" border="0" cellspacing="0" cellpadding="0">
                                <tr>
                                    <td width="247" valign="top">
                                        <div id="othleft-sidebar">
                                            <h1>FILE UPLOADS</h1>
                                            <ul id="yw0">
                                                <li><a class="createfile-upload_ico" active="1" href="@Url.Action("UploadFile","Download")"><span>Create FileUploads</span></a></li>
                                                <li><a class="managfile-upload_ico" active="1" href="@Url.Action("FileUpload_Index","Download")" onclick="setDefault(this.parentNode.id)"><span>Manage FileUploads</span></a></li>
                                            </ul>
                                            <h1>FILE CATOGORY</h1>
                                            <ul>
                                            <li><a class="createfile-catgry_ico" href="@Url.Action("AddCategory","Download")"><span>Create Category</span></a></li>
                                            <li><a class="managefile-catgry_icor_ico" href="@Url.Action("Category_Index","Download")"><span>Manage Category</span></a></li>

                                            </ul>
                                            <div id="subject-name-ajax-grid"></div>



                                        </div>
                                     </td>
                                    <td>
                                        <div class="content" style="        background-color: white">
                                            @RenderBody()
