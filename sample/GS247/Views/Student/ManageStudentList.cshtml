                        $('#CurrentPageIndex').val(1);
                        $('#alphaValue').val('All');
                        GetStudentList('10');
                    };

                    function deleteFilterSearchData(SearchFilterdId) {
                        if (confirm("Are you sure you want to delete this filter?")) {
                            $('#loading').show();
                            $.ajax({
                                type: "POST",
                                url: "/Student/DeleteStudentSelectFilter",
                                data: '{SearchFilterdId: ' + SearchFilterdId + '}',
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                success: function (response) {
                                    var Table = document.getElementById("loadFilter");
                                    Table.innerHTML = "";
                                    var modal = document.getElementById("osload");
                                    modal.style.display = "none";
                                    $('#loading').hide();
                                }
                            });
                        }
                    }

                    function onChangeCourse(ele) {

                        if (ele.value != "") {

                            $('#loading').show();

                            var d = { Id: ele.value };

                            $.ajax({
                                type: "POST",
                                url: "/Student/BatchListLoad",
                                data: JSON.stringify(d),
                                contentType: "application/json;charset=utf-8",
                                dataType: "json",
                                success: function (result) {
                                    $.each(result, function (index, BatchList) {
                                        var Table = document.getElementById("batchid");
                                        Table.innerHTML = "";
                                        if (BatchList.length > 0) {
                                            var value = "<option value=\"0\">All</option>";

                                            for (var i = 0; i < BatchList.length; i++) {
                                                value += "<option value=\"" + BatchList[i].Id + "\">" + BatchList[i].Name + "</option>";
                                            }

                                            $("#batchid").append(value);
                                        }

                                    });
                                },
                                error: function (response) {
                                    alert(response);
                                }
                            });
                            $('#loading').hide();
                        }
                        else
                        {
                            var Table = document.getElementById("batchid");
                            Table.innerHTML = "";
                        }
                    };

                    function onSearchCourseBatch() {
                        var courseId = $('#courseid').val();
                        var batchId = $('#batchid').val();
                        if(courseId != "")
                        {
                            onclickAdvSearch();
                        }
                        else {
                            alert("Please select course");
                        }
                    };

                    function checkBoxCheck() {
                        if ($('.invoice-check').length == $('.invoice-check:checked').length) {
                            $('#checkall').attr('checked', true);
                        }
                        else {
                            $('#checkall').attr('checked', false);
                        }
                    };


                    $('#check-all').change(function () {
                        if ($(this).is(':checked')) {
                            $('.invoice-check').attr('checked', true);
                        }
                        else {
                            $('.invoice-check').attr('checked', false);
                        }
                    });

                    function viewProfileStudent(StudentDetailsId) {
                        window.location.href = '/Student/ViewProfile?StudentDetailsId=' + StudentDetailsId;
                    }

                    function onClose() {
                        $("#myModal").css('display', 'none');
                        $("#myModal1").css('display', 'none');
                    };

</script>

