<script type="text/javascript">

                    $(document).ready(function () {
                        var listsize = $('#listsize').val();
                        $('#CurrentPageIndex').val('1');
                        $('#alphaValue').val('All');
                        GetStudentList(listsize);
                    });

                    function GetStudentList(listsize) {
                        $('#loading').show();
                        var currentPageIndex = $('#CurrentPageIndex').val();
                        var alphaValue = $('#alphaValue').val();
                        var studentAdvanceSearch1 = [];
                        var studentAdvanceSearch = new Object();
                        studentAdvanceSearch.NameTxt = $('#NameTxt').val();
                        studentAdvanceSearch.RollnumberTxt = $('#RollnumberTxt').val();
                        studentAdvanceSearch.AdmissionNumberTxt = $('#AdmissionNumberTxt').val();
                        studentAdvanceSearch.GenderTxt = $('#GenderTxt').val();
                        studentAdvanceSearch.BloodGroupTxt = $('#BloodGroupTxt').val();
                        studentAdvanceSearch.CountryTxt = $('#CountryTxt').val();
                        studentAdvanceSearch.DOBTxt = $('#DOBTxt').val();
                        studentAdvanceSearch.AdmissionDateTxt = $('#AdmissionDateTxt').val();
                        studentAdvanceSearch.StatusTxt = $('#StatusTxt').val();
                        studentAdvanceSearch.AcademicYearTxt = $('#AcademicYearTxt').val();
                        studentAdvanceSearch.DOBDateRange = $('#DOBDateRange').val();
                        studentAdvanceSearch.AdmissionDateRange = $('#AdmissionDateRange').val();
                        studentAdvanceSearch.CourseId = $('#courseid').val();
                        studentAdvanceSearch.BatchId = $('#batchid').val();
                        studentAdvanceSearch1.push(studentAdvanceSearch);

                        var d = { listsize: listsize, currentPageIndex: currentPageIndex, alphaValue: alphaValue, studentAdvanceSearch: studentAdvanceSearch };

                        $.ajax({
                            type: "POST",
                            url: "/Student/GetStudentList",
                            data: JSON.stringify(d),
                            contentType: "application/json;charset=utf-8",
                            dataType: "json",
                            success: function (result) {
                                tableClearValue();
                                $.each(result, function (index, StudentList) {
                                    for (var i = 0; i < StudentList.length; i++) {

                                        $("#StudentListGrid").append(
                        "<tr>"
                        + "<td style=\"text-align:center\"><div class=\"btn-group mailbox-checkall-buttons os_checkbox\"><input class=\"invoice-check\" id=\"" + StudentList[i].StudentDetailsId + "\" type=\"checkbox\" name=\"" + StudentList[i].StudentDetailsId + "\" onclick=\"checkBoxCheck();\"/> </div></td>"
                        + "<td></td>"
                        + "<td><a style=\"color:darkorange\" href=\"javascript:viewProfileStudent(" + StudentList[i].StudentDetailsId + ");\">" + StudentList[i].FirstName + " " + StudentList[i].LastName + "</a></td>"
                        + "<td>" + StudentList[i].AdmissionNo + "</td>"
                        + "<td>" + StudentList[i].SchoolCO.CourseBatchName + "</td>"
                        + "<td></td>"
                        + "<td>" + StudentList[i].SchoolCO.GenderName + "</td>"
                        + "<td align=\"center\"><div class=\"tt-wrapper-new\"><a class=\"makeedit\" title=\"Update\" href=\"javascript:editStudent(" + StudentList[i].StudentDetailsId + ");\"><img src=\"/gridview/update.png\"><span>Edit</span></a>"
                        + "<a class=\"makedelete\" title=\"Delete\" href=\"javascript:deleteStudent(" + StudentList[i].StudentDetailsId + ");\"><img src=\"/gridview/delete.png\"><span>Delete</span></a></div></td>"
                        + "</tr>");
