<script type="text/javascript">

    $(".online-registration_ico").attr('active', '1').closest('li').addClass('list_active');

    function statusChanged(StudentDetailsId, status) {
        $.ajax({
            type: "POST",
            url: "/Student/OnlineUpdateStudentStatus",
            data: '{StudentDetailsId: ' + StudentDetailsId + ',status : ' + status + ',BatchId:null}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                location.reload(true);
            }
        });
    };

    function deleteStudent(StudentDetailsId) {
        if (confirm("Are you sure you want to delete this student?")) {
            $('#loading').show();
            $.ajax({
                type: "POST",
                url: "/Student/DeleteStudent",
                data: '{StudentDetailsId: ' + StudentDetailsId + '}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    location.reload(true);
                }
            });
            $('#loading').hide();
        }
    }

    $(".pageclicktest").click(function () {
        $("#CurrentPage").val($(this).val());
    });

    $(".pageclickNext").click(function () {
        var nextPage = parseInt($("#CurrentPage").val()) + 1;
        $("#CurrentPage").val(nextPage);
    });

    $(".pageclickPrevious").click(function () {
        var prevPage = parseInt($("#CurrentPage").val()) - 1;
        $("#CurrentPage").val(prevPage);
    });

    function onClickAlphabetic(alphaValue) {
        $("#CurrentPage").val('1');
        $('#alphaValue').val(alphaValue);
        document.getElementById("myform").submit();
    };

    function onClickSearch() {
        $("#CurrentPage").val('1');
        $("#alphaValue").val('All');
        document.getElementById("myform").submit();
    };

    function approvalChanged(StudentDetailsId, FirstName, LastName, AdmissionNo, DOB, Batch) {
        var name = FirstName + " " + LastName;
        $('#Name').val(name);
        $('#rId').val(AdmissionNo);
        $('#dob').val(DOB);
        $('#Batch').val(Batch);
        $('#StudentDetailsId').val(StudentDetailsId);
        $('#myModal').css('display', 'block');
    };

    function waitingListChanged(StudentDetailsId, FirstName, LastName, AdmissionNo, DOB, Batch) {
        var name = FirstName + " " + LastName;
        $('#Name1').val(name);
        $('#rId1').val(AdmissionNo);
        $('#dob1').val(DOB);
        $('#Batch1').val(Batch);
        $('#StudentDetailsId').val(StudentDetailsId);
        $('#myModal1').css('display', 'block');
    };

    function onClose() {
        $('#myModal').css('display', 'none');
        $('#myModal1').css('display', 'none');
    };

    function statusApproveChanged(status) {
        $.ajax({
            type: "POST",
            url: "/Student/OnlineUpdateStudentStatus",
            data: '{StudentDetailsId: ' + $('#StudentDetailsId').val() + ',status : ' + status + ',BatchId:' + $('#Batch').val() + '}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                location.reload(true);
            }
        });
    };

    function waitingStatusChanged() {

        var myObject = new Object();
        myObject.StudentDetailsId = $('#StudentDetailsId').val()
        myObject.StarusFlag = 0;
        myObject.Priority = $('#Priority').val()
        myObject.BatchId = $('#Batch1').val()

        var d = { collection: myObject };

        $.ajax({
            type: "POST",
            url: "/Student/OnlineCreateStudentWaiting",
            data: JSON.stringify(d),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                location.reload(true);
            }
        });
    };

    function editWaitingStudent(StudentDetailsId, FirstName, LastName, AdmissionNo, DOB, Batch) {

        var d = { StudentDetailsId: StudentDetailsId };

        $.ajax({
            type: "POST",
            url: "/Student/EditStudentWaiting",
            data: JSON.stringify(d),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (result) {
                if (result.WaitingData != null) {
                    var name = FirstName + " " + LastName;
                    $('#Name1').val(name);
                    $('#rId1').val(AdmissionNo);
                    $('#dob1').val(DOB);
                    $('#Batch1').val(Batch);
                    $('#Priority').val(result.WaitingData.Priority);
                    $('#StudentDetailsId').val(StudentDetailsId);
                    $('#myModal1').css('display', 'block');
                }
            },
            error: function (response) {
                alert('eror');
            }
        });
    };

</script>

