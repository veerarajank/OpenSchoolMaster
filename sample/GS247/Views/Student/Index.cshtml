                <div style="margin-top:20px; width:90%" id="container"></div>*@

                <div class="pdtab_Con" style="width:97%">

                    <div style="font-size:13px; padding:5px 0px"><strong>Recent Active Admissions</strong></div>

                    <table width="100%" border="0" cellspacing="0" cellpadding="0">

                        <tbody>

                            <tr class="pdtab-h">
                                <td align="center" height="18">Date</td>
                                <td align="center" height="18">Roll No</td>
                                <td align="center">Student Name</td>
                                <td align="center">Admission No</td>
                                <td align="center">Course / Batch</td>
                                <td align="center">Gender</td>
                            </tr>
                            @foreach (var item in ViewBag.StudentList)
                            {
                                <tr>
                                    <td align="center">@item.AdmissionDate.ToString("dd MMM yyyy")</td>
                                    <td></td>
                                    <td align="center"><a href="#">@item.FirstName @item.LastName</a></td>
                                    <td align="center">@item.AdmissionNo</td>      
                                    <td align="center">@item.SchoolCO.CourseBatchName</td>      
                                    <td align="center">
                                        @item.SchoolCO.GenderName
                                    </td>                                                                  
                                </tr>
                            }

                    </table>

                </div>

            </div>
        </td>

    </tr>

</table>

